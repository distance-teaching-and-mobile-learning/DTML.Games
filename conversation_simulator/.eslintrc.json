{
  "parser": "babel-eslint",
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-unresolved": [2]
  },
  "globals": {
    "Phaser": true,
    "game": true,
    "gameModule": true,
    "$": true,
    "joint": true,
    "_": true,
    "dtml": true
  },
  "settings": {
    
  }
}
